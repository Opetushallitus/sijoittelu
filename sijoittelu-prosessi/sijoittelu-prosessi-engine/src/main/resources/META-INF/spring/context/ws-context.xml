<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

    <!-- Introduce CXF bus definition -->
    <import resource="classpath:META-INF/cxf/cxf.xml" />
    <import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
    <import resource="classpath:META-INF/cxf/cxf-extension-http.xml" />

    <!-- This file contains all inbound and outbound webservice interfaces -->
    <bean id="tarjontaService" class="fi.vm.sade.tarjonta.service.mock.SijoitteluTarjontaPublicServiceMock" />
    <bean id="valintaperusteService" class="fi.vm.sade.service.valintaperusteet.mock.ValintaperusteMock" />
    <bean id="valintatulosService" class="fi.vm.sade.service.valintatiedot.mock.ValintatulosMock" />

    <!--
    <bean id="sijoitteluAlgorithm" class="fi.vm.sade.process.sijoittelu.mock.SijoitteluAlgorithmMock" />
    -->

    <jaxws:client id="sijoitteluAlgorithm" serviceClass="fi.vm.sade.service.sijoittelu.SijoitteluService"
            address="${algorithm.ws.url}">
    </jaxws:client>

</beans>